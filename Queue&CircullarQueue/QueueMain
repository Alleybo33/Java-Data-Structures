package lab2que;
import java.util.Scanner;
public class QueueApp {

	public static void main(String[] args) {
		
		Scanner scanner = new Scanner(System.in);
		
		QueueX Queue = new QueueX(5);

		
		System.out.println("Please enter 5 transaction IDs.");
		
		QueueX evenQueue = new QueueX(5);
		QueueX oddQueue = new QueueX(5);
		
		for(int i = 0; i < 5; i++) {
			System.out.println("Enter transaction ID " + (i+1) + ": ");
			int transactionId = scanner.nextInt();
			Queue.insert(transactionId);
			if(transactionId % 2 == 0) {
				int val1 = transactionId;
				evenQueue.insert(val1);
				System.out.println("ID " + val1 + " is sent to PC1.");
			} else {
				int val2 = transactionId;
				oddQueue.insert(val2);
				System.out.println("ID " + val2 + " is sent to PC2.");
			}
			
		}
	
		
		System.out.println("PC1");
		while(!evenQueue.isEmpty()) {
			System.out.println("Transaction " + evenQueue.remove());
		}
		
		System.out.println("PC2");
		while(!oddQueue.isEmpty()) {
			System.out.println("Transaction " + oddQueue.remove());
		}
		
		scanner.close();
	}
}
